INIT

!Unmodified ASCII
INCHAR 0 41
  output "A"
INCHAR 0 61
  output "a"

!Ctrl modifier on ASCII letters
INCHAR 4 41
  output "\e[65;5u"
INCHAR 4 61
  output "\x01"

!Alt modifier on ASCII letters
INCHAR 2 41
  output "\eA"
INCHAR 2 61
  output "\ea"

!Ctrl-Alt modifier on ASCII letters
INCHAR 6 41
  output "\e[65;7u"
INCHAR 6 61
  output "\e\x01"
